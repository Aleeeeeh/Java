import java.util.Scanner;

public class Soma {
	public static void main(String args[]) {
		
		Scanner sc = new Scanner(System.in);
		
		int n=sc.nextInt();
		int soma = 0;
		
		for(int i=0; i<n; i++) {
		int x =sc.nextInt();
		soma = soma + i;
		
		}
		System.out.println("Resultado da soma:"+soma);
		sc.close();
	}

}
